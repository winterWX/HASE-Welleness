.col-main{
  display: flex;
  align-items: center;
  border-bottom:1rpx solid #cccc ;
  height: 100rpx;
  padding: 0rpx 30rpx;
  color: rgba(68, 67, 67, 0.8);
}
.leftText{
  width: 300rpx;
  padding-left: 32rpx;
  font-size: 32rpx;
}
.showTextAfter{
  font-size: 25rpx;
  color: #F69DA4;
}
.rightNum{
  display: flex;
  align-items: center;
}
.rightNum .showAfter image{
  width: 60rpx;
  height: 60rpx;
  margin-left: 18rpx;
  margin-top: 12rpx;  
}
.modals {
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.modals-cancel {
  position: absolute;
  z-index: 1000;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
}
.bottom-dialog-body {
  position: absolute;
  z-index: 10001;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 30rpx;
  height: 1100rpx;
  background-color: #fff;
  border-radius: 15rpx 15rpx 0 0;
}
/*动画前初始位置*/
.bottom-pos {
  -webkit-transform: translateY(100%);
  transform: translateY(100%);
}
.userinfo-avatar{
  width: 60rpx;
  height: 60rpx;  
  margin-top:10rpx ;
}
.userinfo-image{
  float: left;
}
.text-content{
  padding-left: 10rpx;
}
.aumsg{
  padding: 120rpx 30rpx 20rpx 30rpx;
  height: 200rpx;  
  border-bottom: 2rpx solid rgba(7,17,27,0.1);
 }
 .msg-box-image{
   text-align: right;
 }
 .msg-box-image image{
  width: 30rpx;
  height: 30rpx;  
 }
 .topTitle{
  font-size: 30rpx;
  color: #C6C6C6;
 }
 .topTitleNumber{
    padding: 10rpx 0rpx 50rpx 0rpx;
    font-weight: 9000;
    color: #303332;  
    font-size: 45rpx;
 }
 .inputValBackNumber{
   margin-right: 10rpx;
 }
 .inputValBack{
    display: flex;
    align-items: center;
    font-size: 32rpx;
 }
 .btn_wrapper{
   display: flex;
   justify-content: center;
   align-items: center;
 }
 .btn_wrapper{
   margin-top:40rpx;
 }
 .btn_wrapper button{
   background-color: #4DCF9C;
   height: 80rpx;
   line-height: 40rpx !important;
   color: #ffffff;
   width: 280px !important;
   border-radius: 50rpx !important;
   font-weight: 700 !important;
   font-size: 32rpx !important;  
 }
